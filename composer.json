{
    "name": "codicastudio/dashboard",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/dashboard",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "divineomega/array_undot": "^4.1",
        "codicastudio/value-widget": "1.0",
        "codicastudio/pill-filter": "1.0",
        "codicastudio/range-filter": "1.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\NovaDashboard\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\NovaDashboard\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
